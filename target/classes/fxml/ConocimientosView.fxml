<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="conocimientosView" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <children>
      <TableView fx:id="conocimientosTable" prefHeight="600.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="140.0" AnchorPane.topAnchor="0.0">
         <columns>
            <TableColumn fx:id="denominacionColumn" prefWidth="150.0" text="Denominación" />
            <TableColumn fx:id="nivelColumn" prefWidth="120.0" text="Nivel" />
         </columns>
      </TableView>
      <Button fx:id="anaComocimientoButton" layoutX="690.0" layoutY="30.0" mnemonicParsing="false" onAction="#onAnadirConocimiento" prefHeight="25.0" prefWidth="130.0" text="Añadir conocimiento" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="0.0" />
      <Button fx:id="anaIdiomaButton" layoutX="690.0" layoutY="70.0" mnemonicParsing="false" onAction="#onAnadirIdioma" prefHeight="25.0" prefWidth="130.0" text="Añadir idioma" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="30.0" />
      <Button fx:id="eliminarButton" layoutX="690.0" layoutY="116.0" mnemonicParsing="false" onAction="#onEliminar" prefHeight="25.0" prefWidth="130.0" text="Eliminar" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="60.0" />
   </children>
</AnchorPane>
