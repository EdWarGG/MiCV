      <Button fx:id="anadirButton" layoutX="701.0" layoutY="14.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onAnadir" prefHeight="25.0" prefWidth="60.0" text="Añadir" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="0.0" />
      <Button fx:id="eliminarButton" layoutX="720.0" layoutY="136.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onEliminar" prefHeight="25.0" prefWidth="60.0" text="Eliminar" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="30.0" />
